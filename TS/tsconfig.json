{
  "compilerOptions": {
    "target": "ES2016",
    "module": "esnext",
    "rootDir": "./",
    "outDir": "../JS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "watchOptions": {
    "watchFile": "fixedpollinginterval"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
